<% if (Array.isArray(theme.widgets) && theme.widgets.length > 0) { %>
<aside id="sidebar">
  <% theme.widgets.forEach(function(widget) { %> <%- partial('widget/' + widget)
  %> <% }) %>
  <div id="toTop" class="fa fa-angle-up"></div>
  <div>
    <a href="https://www.angular.builders/" style="align-content: center"
      ><img
        src="/css/images/angular.builders_cuadrado.png"
        width="100%"
        height="auto"
        alt="Angular Builders"
    /></a>
  </div>
</aside>
<% } %>
